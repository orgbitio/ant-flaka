<project xmlns:c="antlib:it.haefelinger.flaka" xmlns:foo="foobar">
<target name="test-case">

  <c:let>
    i = 0
    n = 9
 </c:let>
  <c:while test=" i lt n ">
    <c:echo>
      #{i}
  </c:echo>
    <c:break test="i>=3" />
    <c:let>
       i = i + 1 
  </c:let>
  </c:while>

</target>
</project>
